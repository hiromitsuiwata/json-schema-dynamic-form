{
  "questions": {
    "$id": "https://example.com/schemas/questions",
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "htmlType": "text",
        "label": "First Name",
        "defaultValue": "John",
        "required": true,
        "order": 1
      },
      "lastName": {
        "type": "string",
        "htmlType": "text",
        "label": "Last Name",
        "defaultValue": "Doe",
        "required": true,
        "order": 2
      },
      "emailAddress": {
        "type": "string",
        "htmlType": "email",
        "label": "Email",
        "required": false,
        "order": 3
      },
      "brave": {
        "type": "options",
        "label": "Bravery Rating",
        "options": [
          {
            "key": "solid",
            "value": "Solid"
          },
          {
            "key": "great",
            "value": "Great"
          },
          {
            "key": "good",
            "value": "Good"
          },
          {
            "key": "unproven",
            "value": "Unproven"
          }
        ],
        "order": 4
      }
    }
  },
  "address": {
    "$id": "https://example.com/schemas/address",
    "type": "object",
    "properties": {
      "street_address": {
        "type": "string",
        "htmlType": "text",
        "label": "street address"
      },
      "city": {
        "type": "string",
        "htmlType": "text",
        "label": "city"
      },
      "state": {
        "type": "string",
        "htmlType": "text",
        "label": "state"
      }
    }
  }
}